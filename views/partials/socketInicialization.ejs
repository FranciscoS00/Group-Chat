<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.0.1/socket.io.js"></script>
<script>
    var socket = io();

    socket.on
    (
        'connect',
        () =>
        {
            console.log('connect');
        }
    )

    socket.on('redirect',
        function (destination){
            window.location.href = destination;
        });
</script>